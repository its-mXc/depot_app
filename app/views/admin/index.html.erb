<%= form_with scope: :admin, method: :get, url: admin_reports_path, local: true do |f| %>
  <%= f.date_field :from %>
  <%= f.date_field :to %>
  <%= f.submit :filter %>
<% end %>
<div>
  <p>
  We have <%= pluralize(@orders.count, "order") %> since <%= pluralize(@days, 'days') %> ago.
</p>
<p><%= @from %></p>
</div>